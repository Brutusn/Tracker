{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./server/src",
    "downlevelIteration": true,
    "outDir": "./compiled_server/src",
    "sourceMap": true,
    "declaration": false,
    "skipLibCheck": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "ES2022",
    "typeRoots": ["node_modules/@types"],
    "lib": ["ES2022", "dom"]
  },
  "include": ["./server/src"]
}
